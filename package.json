{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --fix src/**/*.{ts,vue}",
    "vrt": "rm -rf tests/.reg && reg-suit run",
    "vrt:local": "rm -rf tests/.reg && reg-suit run -c regconfig.local.json",
    "e2e:all": "rm -rf tests/__snapshot__ && playwright test",
    "e2e:chromium": "rm -rf tests/__snapshot__ && playwright test --project=chromium"
  },
  "dependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.26.0",
    "reg-keygen-git-hash-plugin": "^0.14.3",
    "reg-notify-github-plugin": "^0.14.4",
    "reg-suit": "^0.14.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vue-eslint-parser": "^9.4.3"
  }
}
